<?xml version="1.0"?>

<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="airwallex-api" type="host">checkout.airwallex.com</value>
                <value id="metrix" type="host">h.online-metrix.net/</value>
                <value id="static_demo_airwallex" type="host">static-demo.airwallex.com</value>
                <value id="static_airwallex" type="host">static.airwallex.com</value>
                <value id="cdn_scripts_signifyd" type="host">cdn-scripts.signifyd.com</value>
                <value id="bws_demo_airwallex" type="host">bws-demo.airwallex.com</value>
                <value id="bws_airwallex" type="host">bws.airwallex.com</value>
                <value id="script_imgs_signifyd_com" type="host">imgs.signifyd.com</value>
                <value id="allow-online-metrix" type="host">h64.online-metrix.net</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="airwallex-api" type="host">checkout.airwallex.com</value>
                <value id="metrix" type="host">h.online-metrix.net/</value>
                <value id="bws_demo_airwallex_default" type="host">bws-demo.airwallex.com</value>
                <value id="bws_airwallex_default" type="host">bws.airwallex.com</value>
                <value id="api_demo_airwallex_default" type="host">api-demo.airwallex.com</value>
                <value id="api_airwallex_default" type="host">api.airwallex.com</value>
                <value id="signifyd-clear-png" type="host">imgs.signifyd.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="airwallex-api" type="host">checkout.airwallex.com</value>
                <value id="airwallex-demo-api" type="host">checkout-demo.airwallex.com</value>
                <value id="metrix" type="host">h.online-metrix.net/</value>
                <value id="static-demo-airwallex-com" type="host">static-demo.airwallex.com</value>
                <value id="pci-api-demo-airwallex" type="host">pci-api-demo.airwallex.com</value>
                <value id="demo-pacybsmock-airwallex" type="host">demo-pacybsmock.airwallex.com</value>
                <value id="signifyd-clear-com" type="host">imgs.signifyd.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="metrix" type="host">*.online-metrix.net/</value>
                <value id="checkout_airwallex" type="host">checkout.airwallex.com</value>
                <value id="imgs_signifyd_com" type="host">imgs.signifyd.com</value>
                <value id="checkout_demo_airwallex" type="host">checkout-demo.airwallex.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
